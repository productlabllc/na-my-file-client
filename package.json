{
  "name": "myfile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "precommit": "npm run lint",
    "prepare": "husky install",
    "vitest-ui": "vitest --ui",
    "ci-set-npmrc": ". ./params.env && ./build-npmrc.js",
    "ci-deploy": ". ./params.env && ./build-npmrc.js && npm run build && aws s3 sync ./dist/ s3://$UI_BUCKETNAME/ && aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DIST_ID --paths \"/*\"",
    "deploy-dev": "DEPLOYMENT_TARGET=dev . ./params.env && npm run build && aws s3 sync ./dist/ s3://$UI_BUCKETNAME/ && aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DIST_ID --paths \"/*\" && rm -rf ./dist",
    "deploy-prod": "DEPLOYMENT_TARGET=prod . ./params.env && npm run build && aws s3 sync ./dist/ s3://$UI_BUCKETNAME/ && aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DIST_ID --paths \"/*\" && rm -rf ./dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@mui/x-data-grid": "^6.11.0",
    "@mui/x-date-pickers": "^6.18.6",
    "@myfile/api-client": "^1.1.2",
    "amazon-cognito-identity-js": "^6.3.12",
    "date-fns": "^3.2.0",
    "dayjs": "^1.11.10",
    "i18next": "^23.6.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "mui-datatables": "^4.3.0",
    "mui-tel-input": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.3.1",
    "react-markdown": "^8.0.7",
    "react-oidc-context": "^3.0.0",
    "react-pdf": "^7.3.3",
    "react-router-dom": "^6.14.2",
    "react-tooltip": "^5.26.3",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "uuid": "^9.0.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.34.3",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.196",
    "@types/mui-datatables": "^4.3.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.14",
    "esbuild": "0.20.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.24",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^0.34.6"
  }
}
